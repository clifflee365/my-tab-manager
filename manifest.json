{
  "manifest_version": 3,
  "name": "My Tab Manager",
  "version": "1.0",
  "description": "Manage your tabs efficiently",
  "permissions": [
    "tabs",
    "storage",
    "contextMenus"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}